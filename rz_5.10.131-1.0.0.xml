<?xml version="1.0" encoding="UTF-8" ?>
<manifest>

  <!-- default job's -->
  <default revision="dunfell" sync-j="2"/>

  <!-- remote server -->
  <remote name="yocto" fetch="https://git.yoctoproject.org/poky"/>
  <remote name="oe" fetch="https://openembedded.org/meta-openembedded"/>
  <remote name="renesas" fetch="https://github.com/renesas-rz/meta-renesas"/>
  <remote name="gpl" fetch="https://git.yoctoproject.org/meta-gplv2"/>
  <remote name="qt5" fetch="https://github.com/meta-qt5"/>

  <!-- yocto project -->
  <project name="poky" path="source/poky" remote="yocto"/>
  <project name="meta-openembedded" path="source/meta-openembedded" remote="oe" upstream="dunfell"/>
  <project name="meta-gplv2" path="source/meta-gplv2" remote="gpl" upstream="dunfell"/> 
  <project name="meta-qt5" path="source/meta-qt5" remote="qt5" upstream="dunfell"/>
  <project name="meta-renesas" path="source/meta-renesas" remote="renesas" upstream="dunfell/rz"/>

</manifest>



